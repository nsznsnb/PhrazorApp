@page "/genres/create"
@page "/genres/edit/{Id:guid}"
@attribute [Authorize]

<MudContainer>
<SectionTitle Title="@(IsEdit ? "フレーズジャンル編集" : "フレーズジャンル新規作成")" />

<GenreForm Id="Id" IsDialog="false" />
</MudContainer>
@code {
    [Parameter] public Guid? Id { get; set; }

    private bool IsEdit => Id != null;
}