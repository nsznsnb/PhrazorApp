@page "/diary/tags/create"
@page "/diary/tags/edit/{Id:guid}"
@attribute [Authorize]

<PageTitle>日記タグ登録</PageTitle>
<BackLink Href="/diary/tags" />
<SectionTitle Title="@(IsEdit ? "日記タグ編集" : "日記タグ新規作成")" />

<DiaryTagEditor Id="Id" IsDialog="false" />
@code {
    [Parameter] public Guid? Id { get; set; }

    private bool IsEdit => Id != null;
}